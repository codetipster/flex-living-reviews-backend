    "express-rate-limit": "^6.8.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "inversify": "^7.9.1",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.32.1",
