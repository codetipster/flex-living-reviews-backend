    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.6.1",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "sequelize-cli": "^6.6.1",
